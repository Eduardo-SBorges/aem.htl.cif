<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="[/conf/poc.htl.cif/settings/wcm/templates/(?!(landing|root)).*]"
        cq:cifCategoryPage="/content/poc.htl.cif/us/en/products/category-page"
        cq:cifProductPage="/content/poc.htl.cif/us/en/products/product-page"
        cq:cifSearchResultsPage="/content/poc.htl.cif/us/en/search"
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/poc.htl.cif/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        sling:resourceType="poc.htl.cif/components/page"
        navRoot="{Boolean}true"
        pageTitle="aem.htl.cif">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="poc.htl.cif/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="poc.htl.cif/components/container"/>
        </root>
    </jcr:content>
</jcr:root>
